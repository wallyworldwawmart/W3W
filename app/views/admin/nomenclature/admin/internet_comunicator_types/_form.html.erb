<%= semantic_form_for [:admin, @internet_comunicator_type] do |f| %>
  <%= f.error_messages %>

  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :is_email %>
  <% end %>

  <p>
    <%= f.submit t(:create) %> |
    <%= link_to t(:'index', :scope => [:admin, :internet_comunicator_types]), admin_internet_comunicator_types_path %>
  </p>
<% end %>
