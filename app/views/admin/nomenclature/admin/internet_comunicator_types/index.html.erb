<% render :partial => '/admin/navigations/items' %>
<% title t("index", :scope => [:admin, :internet_comunicator_types]) %>

<table>
  <tr>
    <th><%= t("name", :scope => [:admin, :internet_comunicator_types]) %> </th>
  </tr>
  <% for internet_comunicator_type in @internet_comunicator_types %>
    <tr>
      <td><%=h internet_comunicator_type.name %></td>
      <td ><%= link_to image_tag("edit.png", :class => :icon) + h(internet_comunicator_type.name), edit_admin_internet_comunicator_type_path(internet_comunicator_type) %></td>
      <td ><%= link_to t("Delete", :scope =>[:admin, :dashboard]), [:admin, internet_comunicator_type], :confirm => t('Are you sure?'), :method => :delete, :class => "action delete" %></td>
    </tr>
  <% end %>
</table>
<br />
<p>
  <%= link_to  t("add", :scope => [:admin, :internet_comunicator_types]), new_admin_internet_comunicator_type_path,
          :class => "action new" %>
  
</p>
