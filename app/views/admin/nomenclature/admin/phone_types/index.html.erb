<% render :partial => '/admin/navigations/items' %>
<% title t("index", :scope => [:admin, :phone_types]) %>

<table>
  <tr>
    <th><%= t("name", :scope => [:admin, :phone_types]) %></th>
  </tr>
  <% for phone_type in @phone_types %>
    <tr>
      <td><%= link_to image_tag("edit.png", :class => :icon) + h(phone_type.name), edit_admin_phone_type_path(phone_type) %></td>
      <td><%= link_to :delete, :confirm => t('Are you sure?'), :method => :delete, :class => "action delete" %></td>
    </tr>
  <% end %>
</table>
<br />
<p>
  <%= link_to t("add", :scope => [:admin, :phone_types]), new_admin_phone_type_path,
          :class => "action new" %>
  
</p>
