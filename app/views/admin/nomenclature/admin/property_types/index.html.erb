<% render :partial => '/admin/navigations/items' %>
<% title t('.title') %>

<table>
  <tr>
    <th><%= PropertyType.model_name.human(:name => "name")%> </th>
    <th><%= t("status", :scope => [:admin, :property_types])%> </th>
  </tr>
  <% for property_type in @property_types %>
    <tr>
      <td><%= link_to image_tag("edit.png", :class => :icon) + h(property_type.name), edit_admin_property_type_path(property_type) %></td>
      <td><%= property_type.active == true ? t("active", :scope => [:admin, :property_types]) : t("unactive", :scope => [:admin, :property_types]) %></td>
      <td><%= link_to t("Delete", :scope =>[:admin, :dashboard]), [:admin, property_type], :confirm => t('Are you sure?'), :method => :delete, :class => "action delete" %></td>
    </tr>
  <% end %>
</table>
<br />
<p>
  <%= link_to t(".add"), new_admin_property_type_path,
          :class => "action new" %>
  
</p>
