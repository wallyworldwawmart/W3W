<% render :partial => '/admin/navigations/items' %>
<% title t('.title') %>

<table>
  <tr>
    <th><%= StreetType.model_name.human(:name => "name")%> </th>
    <th><%= t("status", :scope => [:admin, :street_types])%> </th>
  </tr>
  <% for street_type in @street_types %>
    <tr>
      <td><%= link_to image_tag("edit.png", :class => :icon) + h(street_type.name), edit_admin_street_type_path(street_type) %></td>
      <td><%= street_type.active == true ? t("active", :scope => [:admin, :street_types]) : t("unactive", :scope => [:admin, :street_types]) %></td>
      <td><%= link_to t("Delete", :scope =>[:admin, :dashboard]), [:admin, street_type], :confirm => t('Are you sure?'), :method => :delete, :class => "action delete" %></td>
    </tr>
  <% end %>
</table>
<br />
<p>
  <%= link_to t(".add"), new_admin_street_type_path,
          :class => "action new" %>
  

</p>
