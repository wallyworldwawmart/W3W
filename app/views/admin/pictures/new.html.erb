<%= semantic_form_for 'picture',
                      :url => admin_contact_sell_pictures_path(:contact_id => @sell.contact.id, :sell_id => @sell.id),
                      :html => { :class => 'nyroModal', :multipart => true} do |f|
%>
    <%= render :partial => 'from', :locals => {:f => f, :picture => @picture} %>

    <%= f.commit_button t(:create)%>
<% end %>
